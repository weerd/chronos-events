{
  "name": "weerd/chronos-events",
  "description": "Laravel package for working with calendar events.",
  "keywords": ["events", "calendar", "cms", "laravel"],
  "license": "MIT",
  "authors": [
    {
      "name": "Diego Lorenzo",
      "email": "diego@diegolorenzo.com",
      "homepage": "http://diegolorenzo.com"
    }
  ],
  "require": {
    "nesbot/carbon": "^1.22",
    "illuminate/support": "^5.4",
    "illuminate/database": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "Weerd\\ChronosEvents\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6"
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Weerd\\ChronosEventsTests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
        "laravel": {
            "providers": [
                "Weerd\\ChronosEvents\\ChronosEventsServiceProvider"
            ]
        }
    }
}
